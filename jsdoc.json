{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./src/utils/"
    ],
    "exclude": [
      "./src/view"
    ]
  },
  "plugins": [],
  "opts": {
    "template": "node_modules/minami",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "verbose": false
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}
